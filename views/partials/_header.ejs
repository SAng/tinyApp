<title>TinyApp</title>



<% if(!username){ %>
    <form action="/login/"" method="POST">
      <label for="login">Enter a Username:</label>
      <input id="login" type="text" name="login">
      <input type="submit" value="Login">
    </form>
  <% } else{ %>
    <%= username %>
    <form action="/logout/"" method="POST">
      <button>Logout</button>
    </form>
<% } %>

